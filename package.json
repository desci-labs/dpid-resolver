{
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "PINO_LOG_LEVEL=silent PORT=5600 mocha --require ts-node/register --recursive --exit ./test/**/*.spec.ts",
        "t": "tsc-alias --watch .",
        "dev": "tsc && (concurrently \"NODE_ENV=develop tsc -w\" \"tsc-alias -w\" \"nodemon --inspect --inspect-port 9309 -q dist/index.js\")",
        "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
        "lint": "eslint . --ext .ts",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/mocha": "^10.0.6",
        "@types/node": "^18.11.19",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "concurrently": "^7.6.0",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "mocha": "^10.2.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.8",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    },
    "dependencies": {
        "@alch/alchemy-web3": "^1.4.7",
        "@desci-labs/desci-contracts": "^0.2.5-rc6",
        "@desci-labs/desci-models": "0.1.8",
        "@supabase/supabase-js": "^2.24.0",
        "axios": "^1.3.2",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "multiformats": "^9.6.4",
        "pino": "^8.14.1",
        "pino-http": "^8.3.3",
        "pino-pretty": "^10.0.0",
        "url-safe-base64": "^1.2.0"
    }
}
